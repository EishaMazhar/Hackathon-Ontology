@prefix : <http://example.org/hackathon#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix hack: <http://example.org/hackathon#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@base <http://example.org/hackathon#> .

<http://example.org/hackathon> rdf:type owl:Ontology ;
                                owl:imports <http://www.w3.org/2004/02/skos/core> ,
                                            foaf: ;
                                dct:creator "Eisha Tir Raazia, UNIGE" ;
                                rdfs:comment "An OWL ontology for hackathons, teams, projects, skills, topics, and awards, reusing FOAF and SKOS." ;
                                owl:versionInfo 1.0 .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/homepage
foaf:homepage rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/name
foaf:name rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://example.org/hackathon#hasAward
hack:hasAward rdf:type owl:ObjectProperty ;
              rdfs:domain hack:Project ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty hack:hasTopic ;
                            owl:someValuesFrom hack:Topic
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty hack:requiresSkill ;
                            owl:someValuesFrom hack:Skill
                          ] ;
              rdfs:range hack:Award .


###  http://example.org/hackathon#hasParticipant
hack:hasParticipant rdf:type owl:ObjectProperty ;
                    owl:inverseOf hack:memberOfTeam ;
                    rdfs:domain hack:Team ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty hack:hasParticipant ;
                                  owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                  owl:onClass hack:Participant
                                ] ;
                    rdfs:range hack:Participant ,
                               foaf:Person .


###  http://example.org/hackathon#hasSkill
hack:hasSkill rdf:type owl:ObjectProperty ;
              rdfs:domain hack:Participant ,
                          foaf:Person ;
              rdfs:range hack:Skill ,
                         skos:Concept ,
                         [ rdf:type owl:Class ;
                           owl:unionOf ( skos:Collection
                                         skos:Concept
                                       )
                         ] .


###  http://example.org/hackathon#hasTeam
hack:hasTeam rdf:type owl:ObjectProperty ;
             owl:inverseOf hack:isTeamOf ;
             rdfs:domain hack:Hackathon ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty hack:hasTeam ;
                           owl:someValuesFrom hack:Team
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty hack:organizedBy ;
                           owl:someValuesFrom hack:Organization
                         ] ;
             rdfs:range hack:Team ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty hack:hasParticipant ;
                          owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                          owl:onClass hack:Participant
                        ] .


###  http://example.org/hackathon#hasTopic
hack:hasTopic rdf:type owl:ObjectProperty ;
              rdfs:range hack:Topic ,
                         skos:Concept ,
                         [ rdf:type owl:Class ;
                           owl:unionOf ( skos:Collection
                                         skos:Concept
                                       )
                         ] .


###  http://example.org/hackathon#isTeamOf
hack:isTeamOf rdf:type owl:ObjectProperty ;
              rdfs:domain hack:Team ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty hack:hasParticipant ;
                            owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                            owl:onClass hack:Participant
                          ] ;
              rdfs:range hack:Hackathon ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty hack:hasTeam ;
                           owl:someValuesFrom hack:Team
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty hack:organizedBy ;
                           owl:someValuesFrom hack:Organization
                         ] .


###  http://example.org/hackathon#memberOfTeam
hack:memberOfTeam rdf:type owl:ObjectProperty ;
                  rdfs:domain hack:Participant ,
                              foaf:Person ;
                  rdfs:range hack:Team ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty hack:hasParticipant ;
                               owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                               owl:onClass hack:Participant
                             ] .


###  http://example.org/hackathon#organizedBy
hack:organizedBy rdf:type owl:ObjectProperty ;
                 rdfs:domain hack:Hackathon ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty hack:hasTeam ;
                               owl:someValuesFrom hack:Team
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty hack:organizedBy ;
                               owl:someValuesFrom hack:Organization
                             ] ;
                 rdfs:range hack:Organization ,
                            foaf:Organization .


###  http://example.org/hackathon#requiresSkill
hack:requiresSkill rdf:type owl:ObjectProperty ;
                   rdfs:domain hack:Project ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty hack:hasTopic ;
                                 owl:someValuesFrom hack:Topic
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty hack:requiresSkill ;
                                 owl:someValuesFrom hack:Skill
                               ] ;
                   rdfs:range hack:Skill ,
                              skos:Concept ,
                              [ rdf:type owl:Class ;
                                owl:unionOf ( skos:Collection
                                              skos:Concept
                                            )
                              ] .


###  http://example.org/hackathon#worksOn
hack:worksOn rdf:type owl:ObjectProperty ;
             rdfs:domain hack:Team ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty hack:hasParticipant ;
                           owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                           owl:onClass hack:Participant
                         ] ;
             rdfs:range hack:Project ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty hack:hasTopic ;
                          owl:someValuesFrom hack:Topic
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty hack:requiresSkill ;
                          owl:someValuesFrom hack:Skill
                        ] .


###  http://www.w3.org/2004/02/skos/core#broadMatch
skos:broadMatch rdfs:subPropertyOf skos:broaderTransitive ,
                                   skos:semanticRelation ;
                rdfs:domain skos:Concept ,
                            [ rdf:type owl:Class ;
                              owl:unionOf ( skos:Collection
                                            skos:Concept
                                          )
                            ] ;
                rdfs:range skos:Concept ,
                           [ rdf:type owl:Class ;
                             owl:unionOf ( skos:Collection
                                           skos:Concept
                                         )
                           ] .


###  http://www.w3.org/2004/02/skos/core#broader
skos:broader rdfs:subPropertyOf skos:semanticRelation ;
             rdfs:domain skos:Concept ,
                         [ rdf:type owl:Class ;
                           owl:unionOf ( skos:Collection
                                         skos:Concept
                                       )
                         ] ;
             rdfs:range skos:Concept ,
                        [ rdf:type owl:Class ;
                          owl:unionOf ( skos:Collection
                                        skos:Concept
                                      )
                        ] .


###  http://www.w3.org/2004/02/skos/core#broaderTransitive
skos:broaderTransitive rdfs:domain skos:Concept ,
                                   [ rdf:type owl:Class ;
                                     owl:unionOf ( skos:Collection
                                                   skos:Concept
                                                 )
                                   ] ;
                       rdfs:range skos:Concept ,
                                  [ rdf:type owl:Class ;
                                    owl:unionOf ( skos:Collection
                                                  skos:Concept
                                                )
                                  ] .


###  http://www.w3.org/2004/02/skos/core#closeMatch
skos:closeMatch rdfs:subPropertyOf skos:semanticRelation ;
                rdfs:domain skos:Concept ,
                            [ rdf:type owl:Class ;
                              owl:unionOf ( skos:Collection
                                            skos:Concept
                                          )
                            ] ;
                rdfs:range skos:Concept ,
                           [ rdf:type owl:Class ;
                             owl:unionOf ( skos:Collection
                                           skos:Concept
                                         )
                           ] .


###  http://www.w3.org/2004/02/skos/core#exactMatch
skos:exactMatch rdfs:subPropertyOf skos:mappingRelation ,
                                   skos:semanticRelation ;
                rdfs:domain skos:Concept ,
                            [ rdf:type owl:Class ;
                              owl:unionOf ( skos:Collection
                                            skos:Concept
                                          )
                            ] ;
                rdfs:range skos:Concept ,
                           [ rdf:type owl:Class ;
                             owl:unionOf ( skos:Collection
                                           skos:Concept
                                         )
                           ] .


###  http://www.w3.org/2004/02/skos/core#hasTopConcept
skos:hasTopConcept rdfs:range [ rdf:type owl:Class ;
                                owl:unionOf ( skos:Collection
                                              skos:Concept
                                            )
                              ] .


###  http://www.w3.org/2004/02/skos/core#mappingRelation
skos:mappingRelation rdfs:domain skos:Concept ,
                                 [ rdf:type owl:Class ;
                                   owl:unionOf ( skos:Collection
                                                 skos:Concept
                                               )
                                 ] ;
                     rdfs:range skos:Concept ,
                                [ rdf:type owl:Class ;
                                  owl:unionOf ( skos:Collection
                                                skos:Concept
                                              )
                                ] .


###  http://www.w3.org/2004/02/skos/core#member
skos:member rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( skos:Collection
                                        skos:Concept
                                      )
                        ] .


###  http://www.w3.org/2004/02/skos/core#memberList
skos:memberList rdfs:domain skos:Collection ,
                            [ rdf:type owl:Class ;
                              owl:unionOf ( skos:Collection
                                            skos:Concept
                                          )
                            ] .


###  http://www.w3.org/2004/02/skos/core#narrowMatch
skos:narrowMatch rdfs:subPropertyOf skos:narrowerTransitive ,
                                    skos:semanticRelation ;
                 rdfs:domain skos:Concept ,
                             [ rdf:type owl:Class ;
                               owl:unionOf ( skos:Collection
                                             skos:Concept
                                           )
                             ] ;
                 rdfs:range skos:Concept ,
                            [ rdf:type owl:Class ;
                              owl:unionOf ( skos:Collection
                                            skos:Concept
                                          )
                            ] .


###  http://www.w3.org/2004/02/skos/core#narrower
skos:narrower rdfs:subPropertyOf skos:semanticRelation ;
              rdfs:domain skos:Concept ,
                          [ rdf:type owl:Class ;
                            owl:unionOf ( skos:Collection
                                          skos:Concept
                                        )
                          ] ;
              rdfs:range skos:Concept ,
                         [ rdf:type owl:Class ;
                           owl:unionOf ( skos:Collection
                                         skos:Concept
                                       )
                         ] .


###  http://www.w3.org/2004/02/skos/core#narrowerTransitive
skos:narrowerTransitive rdfs:domain skos:Concept ,
                                    [ rdf:type owl:Class ;
                                      owl:unionOf ( skos:Collection
                                                    skos:Concept
                                                  )
                                    ] ;
                        rdfs:range skos:Concept ,
                                   [ rdf:type owl:Class ;
                                     owl:unionOf ( skos:Collection
                                                   skos:Concept
                                                 )
                                   ] .


###  http://www.w3.org/2004/02/skos/core#related
skos:related rdfs:domain skos:Concept ,
                         [ rdf:type owl:Class ;
                           owl:unionOf ( skos:Collection
                                         skos:Concept
                                       )
                         ] ;
             rdfs:range skos:Concept ,
                        [ rdf:type owl:Class ;
                          owl:unionOf ( skos:Collection
                                        skos:Concept
                                      )
                        ] .


###  http://www.w3.org/2004/02/skos/core#relatedMatch
skos:relatedMatch rdfs:subPropertyOf skos:semanticRelation ;
                  rdfs:domain skos:Concept ,
                              [ rdf:type owl:Class ;
                                owl:unionOf ( skos:Collection
                                              skos:Concept
                                            )
                              ] ;
                  rdfs:range skos:Concept ,
                             [ rdf:type owl:Class ;
                               owl:unionOf ( skos:Collection
                                             skos:Concept
                                           )
                             ] .


###  http://www.w3.org/2004/02/skos/core#semanticRelation
skos:semanticRelation rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( skos:Collection
                                                  skos:Concept
                                                )
                                  ] ;
                      rdfs:range [ rdf:type owl:Class ;
                                   owl:unionOf ( skos:Collection
                                                 skos:Concept
                                               )
                                 ] .


###  http://www.w3.org/2004/02/skos/core#topConceptOf
skos:topConceptOf rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( skos:Collection
                                              skos:Concept
                                            )
                              ] .


#################################################################
#    Data properties
#################################################################

###  http://example.org/hackathon#deadline
hack:deadline rdf:type owl:DatatypeProperty ;
              rdfs:domain hack:Hackathon ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty hack:hasTeam ;
                            owl:someValuesFrom hack:Team
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty hack:organizedBy ;
                            owl:someValuesFrom hack:Organization
                          ] ;
              rdfs:range xsd:dateTime .


###  http://example.org/hackathon#endDate
hack:endDate rdf:type owl:DatatypeProperty ;
             rdfs:domain hack:Hackathon ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty hack:hasTeam ;
                           owl:someValuesFrom hack:Team
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty hack:organizedBy ;
                           owl:someValuesFrom hack:Organization
                         ] ;
             rdfs:range xsd:dateTime .


###  http://example.org/hackathon#githubUrl
hack:githubUrl rdf:type owl:DatatypeProperty ;
               rdfs:domain hack:Project ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty hack:hasTopic ;
                             owl:someValuesFrom hack:Topic
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty hack:requiresSkill ;
                             owl:someValuesFrom hack:Skill
                           ] ;
               rdfs:range xsd:anyURI .


###  http://example.org/hackathon#startDate
hack:startDate rdf:type owl:DatatypeProperty ;
               rdfs:domain hack:Hackathon ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty hack:hasTeam ;
                             owl:someValuesFrom hack:Team
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty hack:organizedBy ;
                             owl:someValuesFrom hack:Organization
                           ] ;
               rdfs:range xsd:dateTime .


#################################################################
#    Classes
#################################################################

###  http://example.org/hackathon#Award
hack:Award rdf:type owl:Class ;
           owl:disjointWith hack:Hackathon ,
                            hack:Organization ,
                            hack:Participant ,
                            hack:Project ,
                            hack:Skill ,
                            hack:Team ,
                            hack:Topic .


###  http://example.org/hackathon#DataScienceTeam
hack:DataScienceTeam rdf:type owl:Class ;
                     rdfs:subClassOf hack:Team ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty hack:hasParticipant ;
                                       owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                       owl:onClass hack:Participant
                                     ] .


###  http://example.org/hackathon#Hackathon
hack:Hackathon rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty hack:hasTeam ;
                                 owl:someValuesFrom hack:Team
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty hack:organizedBy ;
                                 owl:someValuesFrom hack:Organization
                               ] ;
               owl:disjointWith hack:Organization ,
                                hack:Participant ,
                                hack:Project ,
                                hack:Skill ,
                                hack:Team ,
                                hack:Topic .


###  http://example.org/hackathon#Judge
hack:Judge rdf:type owl:Class ;
           rdfs:subClassOf foaf:Person .


###  http://example.org/hackathon#Mentor
hack:Mentor rdf:type owl:Class ;
            rdfs:subClassOf foaf:Person .


###  http://example.org/hackathon#Organization
hack:Organization rdf:type owl:Class ;
                  rdfs:subClassOf foaf:Organization ;
                  owl:disjointWith hack:Participant ,
                                   hack:Project ,
                                   hack:Skill ,
                                   hack:Team ,
                                   hack:Topic .


###  http://example.org/hackathon#Participant
hack:Participant rdf:type owl:Class ;
                 rdfs:subClassOf foaf:Person ;
                 owl:disjointWith hack:Project ,
                                  hack:Skill ,
                                  hack:Team ,
                                  hack:Topic .


###  http://example.org/hackathon#Project
hack:Project rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty hack:hasTopic ;
                               owl:someValuesFrom hack:Topic
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty hack:requiresSkill ;
                               owl:someValuesFrom hack:Skill
                             ] ;
             owl:disjointWith hack:Skill ,
                              hack:Team ,
                              hack:Topic .


###  http://example.org/hackathon#Skill
hack:Skill rdf:type owl:Class ;
           rdfs:subClassOf skos:Concept ,
                           [ rdf:type owl:Class ;
                             owl:unionOf ( skos:Collection
                                           skos:Concept
                                         )
                           ] ;
           owl:disjointWith hack:Team ,
                            hack:Topic .


###  http://example.org/hackathon#Team
hack:Team rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty hack:hasParticipant ;
                            owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                            owl:onClass hack:Participant
                          ] ;
          owl:disjointWith hack:Topic .


###  http://example.org/hackathon#Topic
hack:Topic rdf:type owl:Class ;
           rdfs:subClassOf skos:Concept ,
                           [ rdf:type owl:Class ;
                             owl:unionOf ( skos:Collection
                                           skos:Concept
                                         )
                           ] .


###  http://example.org/hackathon#WellBalancedTeam
hack:WellBalancedTeam rdf:type owl:Class ;
                      rdfs:subClassOf hack:Team ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty hack:hasParticipant ;
                                        owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                        owl:onClass hack:Participant
                                      ] .


###  http://www.w3.org/2004/02/skos/core#Collection
skos:Collection rdfs:subClassOf [ rdf:type owl:Class ;
                                  owl:unionOf ( skos:Collection
                                                skos:Concept
                                              )
                                ] .


###  http://www.w3.org/2004/02/skos/core#Concept
skos:Concept rdfs:subClassOf [ rdf:type owl:Class ;
                               owl:unionOf ( skos:Collection
                                             skos:Concept
                                           )
                             ] .


###  http://www.w3.org/2004/02/skos/core#OrderedCollection
skos:OrderedCollection rdfs:subClassOf [ rdf:type owl:Class ;
                                         owl:unionOf ( skos:Collection
                                                       skos:Concept
                                                     )
                                       ] .


###  http://xmlns.com/foaf/0.1/Organization
foaf:Organization rdf:type owl:Class .


###  http://xmlns.com/foaf/0.1/Person
foaf:Person rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  http://example.org/hackathon#AwardBestHealth
hack:AwardBestHealth rdf:type owl:NamedIndividual ,
                              hack:Award ;
                     dct:title "Best Health Innovation"@en .


###  http://example.org/hackathon#AwardBestSustainability
hack:AwardBestSustainability rdf:type owl:NamedIndividual ,
                                      hack:Award ;
                             dct:title "Best Sustainability Project"@en .


###  http://example.org/hackathon#AwardJuryMention
hack:AwardJuryMention rdf:type owl:NamedIndividual ,
                               hack:Award ;
                      dct:title "Jury Special Mention"@en .


###  http://example.org/hackathon#HackathonClimate2025
hack:HackathonClimate2025 rdf:type owl:NamedIndividual ,
                                   hack:Hackathon ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty hack:hasTeam ;
                                     owl:someValuesFrom hack:Team
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty hack:hasTopic ;
                                     owl:someValuesFrom hack:Topic
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty hack:organizedBy ;
                                     owl:someValuesFrom hack:Organization
                                   ] ;
                          hack:hasTeam hack:TeamCityGuard ;
                          hack:hasTopic hack:TopicSDG11 ;
                          hack:organizedBy hack:OrgEPFL ;
                          hack:deadline "2025-09-05T23:59:00"^^xsd:dateTime ;
                          hack:endDate "2025-09-17T18:00:00"^^xsd:dateTime ;
                          hack:startDate "2025-09-15T09:00:00"^^xsd:dateTime ;
                          dct:description "Urban climate resilience and flood monitoring"@en ;
                          dct:title "Climate Resilience Hackathon 2025"@en .


###  http://example.org/hackathon#HackathonHealth2025
hack:HackathonHealth2025 rdf:type owl:NamedIndividual ,
                                  hack:Hackathon ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty hack:hasTeam ;
                                    owl:someValuesFrom hack:Team
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty hack:hasTopic ;
                                    owl:someValuesFrom hack:Topic
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty hack:organizedBy ;
                                    owl:someValuesFrom hack:Organization
                                  ] ;
                         hack:hasTeam hack:TeamInfinity ;
                         hack:hasTopic hack:TopicSDG3 ;
                         hack:organizedBy hack:OrgCERN ;
                         hack:deadline "2025-10-01T23:59:00"^^xsd:dateTime ;
                         hack:endDate "2025-10-12T18:00:00"^^xsd:dateTime ;
                         hack:startDate "2025-10-10T09:00:00"^^xsd:dateTime ;
                         dct:description "Solutions for good health and well-being"@en ;
                         dct:title "HealthTech for SDG3 Hackathon 2025"@en .


###  http://example.org/hackathon#HackathonSDG2025
hack:HackathonSDG2025 rdf:type owl:NamedIndividual ,
                               hack:Hackathon ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty hack:hasTeam ;
                                 owl:someValuesFrom hack:Team
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty hack:hasTopic ;
                                 owl:someValuesFrom hack:Topic
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty hack:organizedBy ;
                                 owl:someValuesFrom hack:Organization
                               ] ;
                      hack:hasTeam hack:TeamDataHeroes ,
                                   hack:TeamInfinity ;
                      hack:hasTopic hack:TopicAI4Good ;
                      hack:organizedBy hack:OrgUNIGE ;
                      hack:deadline "2025-11-20T23:59:00"^^xsd:dateTime ;
                      hack:endDate "2025-11-30T18:00:00"^^xsd:dateTime ;
                      hack:startDate "2025-11-28T09:00:00"^^xsd:dateTime ;
                      dct:description "Hackathon on sustainable development goals"@en ;
                      dct:title "SDG Innovation Hackathon 2025"@en .


###  http://example.org/hackathon#OrgCERN
hack:OrgCERN rdf:type owl:NamedIndividual ,
                      hack:Organization ,
                      foaf:Organization ;
             foaf:homepage <https://home.cern> ;
             foaf:name "CERN" .


###  http://example.org/hackathon#OrgEPFL
hack:OrgEPFL rdf:type owl:NamedIndividual ,
